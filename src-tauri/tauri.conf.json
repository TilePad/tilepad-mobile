{
  "$schema": "https://schema.tauri.app/config/2",
  "mainBinaryName": "tilepad-mobile",
  "productName": "Tilepad Mobile",
  "version": "0.1.0",
  "identifier": "com.jacobtread.tilepad.mobile",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1430",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "tilepad-mobile",
        "width": 800,
        "height": 600,
        "useHttpsScheme": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../THIRD_PARTY_LICENSES.md": "THIRD_PARTY_LICENSES.md"
    }
  }
}